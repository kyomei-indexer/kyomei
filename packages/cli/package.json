{
  "name": "@kyomei/cli",
  "version": "0.0.1",
  "description": "Command-line interface for Kyomei EVM indexer",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "kyomei": "./dist/bin.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@kyomei/config": "workspace:*",
    "@kyomei/core": "workspace:*",
    "@kyomei/database": "workspace:*",
    "@kyomei/runner": "workspace:*",
    "@aws-sdk/client-s3": "^3.705.0",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
