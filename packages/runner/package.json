{
  "name": "@kyomei/runner",
  "version": "0.0.1",
  "description": "Service orchestration for all-in-one or distributed deployment",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@kyomei/config": "workspace:*",
    "@kyomei/core": "workspace:*",
    "@kyomei/database": "workspace:*",
    "@kyomei/syncer": "workspace:*",
    "@kyomei/processor": "workspace:*",
    "@kyomei/cron": "workspace:*",
    "@kyomei/api": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
